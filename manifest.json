{
    "manifest_version": 2,
    "name": "My Age Timer",
    "version": "1.0",
    "description": "Displays your age and a synced notepad.",
    "permissions": ["storage"],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "my-age-timer@example.com"
      }
    }
  }
  